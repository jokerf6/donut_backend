{
  "name": "planner",
  "version": "0.0.1",
  "description": "planner app",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "cross-env NODE_ENV=production && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production && node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:generate": "prisma generate",
    "db:mix": "yarn prismix && yarn prisma format && yarn db:generate",
    "db:push": "npx prisma db push --skip-generate",
    "db:sync": "yarn db:mix && yarn db:push"
  },
  "dependencies": {
    "@googleapis/calendar": "^9.7.1",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.1",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.1.17",
    "@prisma/client": "^5.6.0",
    "@types/multer": "^1.4.7",
    "@types/speakeasy": "^2.0.7",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "bcrypt-nodejs": "^0.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "date-holidays": "^3.23.11",
    "dotenv": "^16.4.1",
    "google-auth-library": "^9.6.3",
    "googleapis": "^136.0.0",
    "inquirer": "^9.2.14",
    "jsonwebtoken": "^9.0.1",
    "moment": "^2.30.1",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "public-holidays": "^0.2.2",
    "puppeteer": "^22.5.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "speakeasy": "^2.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.2.4",
    "@types/node": "18.11.18",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.3.1",
    "prettier": "^2.3.2",
    "prisma": "^5.9.1",
    "prismix": "jokerf6/prismix",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
